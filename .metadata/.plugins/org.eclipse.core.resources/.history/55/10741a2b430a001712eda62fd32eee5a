class SyntaxDDL:
    _instances = None
    def __new__(cls, *args, **kwargs):
        if cls not in cls._instances:
            cls._instances[cls] = super(SyntaxDDL, cls).__new__(*args, **kwargs)
        return cls._instances[cls]
    
    def createSyntaxDDL(self):
        pass
    
    
    