from NoSQL_Invetory.SchemaData import SchemaData

class SchemaDataDocument(SchemaData):
    
    def __init__(self):
        self.documents = []
        self.schemas = []
    
    def addSchema(self, name):
        SchemaData.addSchema(self, name)
        
    def addDocument(self, document):
        self.documents.append(document)
        